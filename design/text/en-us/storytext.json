      "StoryTextDay00CantExit":{
         "dialogTitle":"OBJ_NPC_LISA",
         "text":"It's not time for your break yet.",
         "align":2
         },
      "StoryTextDay00NPC0Customers":{
         "dialogTitle":"OBJ_NPC_LISA",
         "text":"We really need to make some sales.|Please help each customer.",
         "align":2
